						height 500px 
						overflow-y scroll !important
.check-boletas
	margin-left 15
	margin-right 10
.bloque
	background-color color-base
	-webkit-print-color-adjust exact
	padding 1em 0
	margin-top 5px
	max-height 200px
	&:hover
		background-color #008080
		-webkit-print-color-adjust exact
		cursor pointer
	
	&.no-active
		border 2px double red !important

		&:hover
			background-color color-base
			-webkit-print-color-adjust exact
			cursor default

	i
		color #fff
		display block
		font-size 52px
		text-align center
		padding-top 20px

	span
		color #fff
		display block
		margin 0 auto
		text-align center
		word-wrap break-word
	&.no-hover:hover
		background-color color-base !important
		-webkit-print-color-adjust exact
			
