.titulo
	margin-top 15px !important
	h3
		color #000
		font-family 'Roboto', sans-serif
		font-size 30px !important
		font-weight 300 !important
	span.subTitulo
		color gray !important
	i
		color #f9df0c !important
		margin-right 20px !important
		margin-left 32px

.block
	display block
	width 100%




.row
	&.custom
		max-width 80em !important

.border
	border 1px solid blue
.form-custom
	background-color rgba(255, 255, 255, 0.8)

	input
		height 1.9rem
		margin 0 0 5px 0 !important

.button
	box-shadow 1px 1px 5px #333
	font-weight bold
	margin 1em 0.3em 0
	text-shadow 1px 1px 5px #333

	&.special
		margin 0 !important

	&.align-top
		margin-top 20px
	&.fill
		width 100% !important
.pricing-table
	&.pricing-custom
		.bullet-item
			text-align left !important
			i
				display inline-block
				width 9%
				&::after
					content 'Incompleto'

				&.completo
					color #008080
					font-weight bold
					font-style normal
					text-align center !important
					
					&::after
						content 'Completo'

				&.incompleto
					color #C10000
					font-weight bold
					font-style normal
					text-align center !important
					
					&::after
						content 'Incompleto'

			span
				display inline-block
				width 89%

		&.complete
			color red

.table
	tbody

		&.custom-inputs

			td
				clear()
				height 25px

				input
					clear()
					margin-top 5px !important
					height 25px
.table-custom-parciales
	tbody
		tr
			td
				&:hover
					background-color #008080
					-webkit-print-color-adjust exact
					cursor pointer
					color #fff
				&.active
					background-color #008080
					-webkit-print-color-adjust exact
					color #fff
.tabs
	&.custom
		dd,
		li
			a
				background-color color-base
				border-bottom 1px solid #EEE
				color #FEFEFE

			&.active,
			&:hover
				a
					background-color color-hover
					color #FEFEFE
					font-weight bold
					text-shadow 3px 3px 10px #333
