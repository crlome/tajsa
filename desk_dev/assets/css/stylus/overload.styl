		display inline-block
.contenedorScroll-y
	overflow-y hidden !important
	overflow-y scroll !important
	max-height 500px
	height 500px
	border 1px solid #fff !important
.btn	
	border-radius 50px !important
	box-shadow none !important
	border 1px solid #fff !important
	height 50px !important
	min-width 100px !important
	width auto !important
	color #000 !important
	padding-top 15px !important
	background-image none !important
	text-shadow none !important
	&.btn-success
		background-color #3af577 !important
	&.btn-primary
		background-color #18e3e0 !important	
	&.btn-danger
		background-color #ff3535 !important
	&.btn-save
		background-color #f9df0c !important
	&.fill
		width 100% !important
	&.btn-90
		border-radius 0px !important
.gvDatos
	width 100% !important
	margin-bottom 50px !important
.bb-grid 
	margin 0 auto
	thead
		border-bottom 3px solid #f9df0c !important
		background-color rgba(0, 0, 0, .0)
		tr
			th
				border 1px solid rgba(0, 0, 0, .0)
				strong
					span
						color #000
						font-size 14px !important
	tbody
		tr
			border 1px solid rgba(0, 0, 0, .0)
			border-bottom 1px solid #f1f1f1 !important
			color #9d988d !important
			font-size 14px !important		
			td
				padding 10px !important
		span
			color #333

		.gv-order 
			cursor  pointer
			
			&.gv-asc strong span::after 
				content  icon-asc
				float  right
				font-family  'icomoon'
				font-size  1em
				font-weight  normal !important
			
			&.gv-desc strong span::after 
				content  icon-desc
				float  right
				font-family  'icomoon'
				font-size  1em
				font-weight  normal !important
				
			input 
				height  1.7em
				font-weight  normal !important
			
		.gv-col-command::before 
			content  icon-settings
			font-family  'icomoon'
			font-size  1.5em
			font-weight  normal
	
	tbody
		border 1px solid #DDD
		
		input[type="checkbox"] 
			border 1px solid blue !important
			display  inline-block
			height  80% !important
			margin-top  1em
			width  80% !important
			
		tr
			cursor  pointer !important
			&.isSelected
				background-color #ffffff

				td
					color #000
					font-weight bold			
			
	tfoot
		border 1px solid transparent
		background-color white
		
		tr 
			strong
				span
					margin-top 5px !important
			td
				font-weight normal !important

		.gv-all::before 
			content  icon-all
			font-family  'icomoon'
			font-size  1.7em
			
		.gv-begin::before 
			content  icon-begin
			font-family  'icomoon'
			font-size  1.7em

		.gv-end::before 
			content  icon-end
			font-family  'icomoon'
			font-size  1.7em
			
		.gv-next::before 
			content  icon-nextCustom
			font-family  'icomoon'
			font-size  1.7em

		.gv-none::before 
			content  icon-calendar
			font-family  'icomoon'
			font-size  1.7em
			
		.gv-pager
			
			.columns
				height 30px
				padding-top 5px
						
			span,
			input 
				color  #333
				display  inline-block !important
				line-height  25px
				vertical-align  top
			a
				color #000 !important
				background-color #f9df0c
				padding-top 5px
				padding-left 5px
				border-radius 50px
				padding-right 5px
			a:hover
				color  #000
				background-color #f9df0c
				padding-top 5px
				padding-left 5px
				border-radius 50px
				padding-right 5px
				text-decoration none !important
				transition-duration  0.5s

		.gv-pages
			height  25px !important
			width  50px !important
			margin-top 6px !important

		.gv-prev
			margin-top 0px !important	

			&::before 
				content  icon-prevCustom
				font-family  'icomoon'
				font-size  1.7em
.gv-filter
	border 1px solid rgba(0, 0, 0, .0)	
	&::-webkit-input-placeholder
		color #cecece !important
		font-weight 400
		&::before
			font-family 'icomoon'
			content icon-search
