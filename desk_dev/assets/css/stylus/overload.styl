.accordion
	&.accordion-custom
		dd.accordion-navigation		
			a
				background-color color-base
				-webkit-print-color-adjust exact
				color #fff
				border-bottom solid #fff
				&:hover
					background-color color-hover
			&.active
				a
					background-color color-hover
			div
				&.content
					border 1px solid #C0C0C0
					background-color #EAEAEA
					-webkit-print-color-adjust exact					
